Title>\n          <img src={logo} alt=\"logo\" />\n          <p>\n            Móveis planejados <br />\n            Entrar no sistema\n          </p>\n        </Title>\n        <ErrorContainer>\n          <NotFoundError error={error}>{error?.data.error}</NotFoundError>\n          <NoInputError error={values.inputErr}>{values.inputErr}</NoInputError>\n        </ErrorContainer>\n        <Input\n          name=\"username\"\n          placeholder=\"Username\"\n          title=\"Digite um Username\"\n          type=\"text\"\n          error={values.inputErr}\n          value={values.username}\n          onChange={handleInputChange}\n        />\n        <Input\n          name=\"password\"\n          placeholder=\"Password\"\n          title=\"Digite um Password\"\n          type=\"password\"\n          error={values.inputErr}\n          value={values.password}\n          onChange={handleInputChange}\n        />\n        <Button\n          type=\"submit\"\n          title=\"Clique para entrar no sistema\"\n          disabled={loading}\n        >\n          {loading ? <LoaderSpinner title=\"Aguarde...\" /> : \"Entrar\"}\n        </Button>\n        <ActionContainer>\n          <p title=\"Ainda não implementado\">Esqueceu sua senha?</p>\n          <Link to=\"/signup\" title=\"Crie uma conta para entrar no sistema\">\n            Criar uma conta\n          </Link>\n        </ActionContainer>\n      </Form>\n      <FormFooter>\n        <p>Leonardo & Jorge</p>\n      </FormFooter>\n    </Container>\n  );\n};\n\nexport default SignIn;\n","/home/osvaldo/Documentos/code/roughwood/roughwood-web/src/components/LoaderSpinner/styles.js",[],"/home/osvaldo/Documentos/code/roughwood/roughwood-web/src/Pages/SignIn/styles.js",[],"/home/osvaldo/Documentos/code/roughwood/roughwood-web/src/styles/global.js",[],"/home/osvaldo/Documentos/code/roughwood/roughwood-web/src/Layout/index.js",[],"/home/osvaldo/Documentos/code/roughwood/roughwood-web/src/styles/themes/light.js",["159"],"export default {\n  title: 'light',\n\n  colors: {\n    primary: '#54361b',\n    secondary: '#d4a37b',\n\n    background: '#E9EDC9',\n    text: '#333',\n    textReverse: '#ebeddf',\n  }\n};\n","/home/osvaldo/Documentos/code/roughwood/roughwood-web/src/index.js",[],["160","161"],"/home/osvaldo/Documentos/code/roughwood/roughwood-web/src/Routes/index.js",[],"/home/osvaldo/Documentos/code/roughwood/roughwood-web/src/Pages/User/index.js",[],"/home/osvaldo/Documentos/code/roughwood/roughwood-web/src/Pages/SignUp/styles.js",[],"/home/osvaldo/Documentos/code/roughwood/roughwood-web/src/Pages/SignUp/index.js",["162"],"/home/osvaldo/Documentos/code/roughwood/roughwood-web/src/components/Header/index.js",["163"],"/home/osvaldo/Documentos/code/roughwood/roughwood-web/src/contexts/auth.js",["164"],"import { createContext, useState, useEffect, useCallback } from \"react\";\n\nimport api from '../services/api';\nimport { signInService, signUpService } from \"../services/auth\";\nimport useLocalStorage from \"../hooks/useLocalStorage\";\n\nconst AuthContext = createContext({});\n\nexport const AuthProvider = ({ children }) => {\n  const [user, setUser] = useState(null);\n  const [error, setError] = useState(null);\n  const [loading, setLoading] = useState(true);\n  const [storageUser, setStorageUser] = useLocalStorage('@roughwood:user');\n  const [storageToken, setStorageToken] = useLocalStorage('@roughwood:token');\n\n  useEffect(() => {\n    if (storageUser && storageToken) {\n      setUser(storageUser);\n      api.defaults.headers.Authorization = `Baerer ${storageToken}`;\n    }\n    setLoading(false);\n  }, [storageUser, storageToken]);\n\n  const signIn = useCallback(async ({ username, password }) => {\n    setLoading(true);\n    setError(null);\n    try {\n      const { user, token } = await signInService({ username, password });\n      setUser(user);\n      api.defaults.headers.Authorization = `Baerer ${token}`;\n      setStorageUser(user);\n      setStorageToken(token);\n    } catch (err) {\n      setError(err.response);\n    } finally {\n      setLoading(false);\n    }\n  }, [setStorageUser, setStorageToken]);\n\n  const signUp = useCallback(async ({ name, username, password }) => {\n    setLoading(true);\n    setError(null);\n    try {\n      await signUpService({ name, username, password });\n\n      signIn({ username, password });\n    } catch (err) {\n      setError(err.response);\n      setLoading(false);\n    }\n  }, [signIn]);\n\n  const signOut = useCallback(async () => {\n    await new Promise(resolve => setTimeout(resolve, 100));\n    setLoading(true);\n    setError(null);\n    setStorageUser(null);\n    setStorageToken(null);\n    setUser(null);\n    setLoading(false);\n  }, [setStorageUser, setStorageToken]);\n\n  const isUserSignedIn = !!user;\n\n  return (\n    <AuthContext.Provider value={{ isUserSignedIn, user, signIn, signUp, signOut, loading, error }}>\n      {children}\n    </AuthContext.Provider>\n  );\n};\n\nexport default AuthContext;\n","/home/osvaldo/Documentos/code/roughwood/roughwood-web/src/services/api.js",[],"/home/osvaldo/Documentos/code/roughwood/roughwood-web/src/Layout/styles.js",[],"/home/osvaldo/Documentos/code/roughwood/roughwood-web/src/components/Header/styles.js",[],"/home/osvaldo/Documentos/code/roughwood/roughwood-web/src/components/Header/Footer/index.js",[],"/home/osvaldo/Documentos/code/roughwood/roughwood-web/src/components/Header/Footer/styles.js",[],"/home/osvaldo/Documentos/code/roughwood/roughwood-web/src/components/Header/Profile/index.js",[],"/home/osvaldo/Documentos/code/roughwood/roughwood-web/src/components/Header/Profile/styles.js",[],"/home/osvaldo/Documentos/code/roughwood/roughwood-web/src/components/Header/Menu/index.js",[],"/home/osvaldo/Documentos/code/roughwood/roughwood-web/src/components/Header/Menu/styles.js",[],"/home/osvaldo/Documentos/code/roughwood/roughwood-web/src/Pages/Dashboard/index.js",[],"/home/osvaldo/Documentos/code/roughwood/roughwood-web/src/hooks/useLocalStorage.js",[